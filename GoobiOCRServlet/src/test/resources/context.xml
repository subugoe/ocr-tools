<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="OCREngineFactory" name="OCREngineFactory"
		class="de.uni_goettingen.sub.commons.ocr.api.OCREngineFactory"
		factory-method="getInstance">
		<property name="engine" ref="testServerOCREngine" />
	</bean>

	

	<bean id="testServerOCREngine" name="testServerOCREngine"
		class="de.unigoettingen.sub.commons.ocr.servlet.OCRTestEngine"
		factory-method="getInstance">
	</bean>



</beans>